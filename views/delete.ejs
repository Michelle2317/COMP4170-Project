<%- include('../partials/header') %>
<body>
	<header>
		<h1>Delete Entry</h1>
	</header>
	<div class="delete-h1">
		<h1>
			Are you sure you want to delete this entry "<%=
			entryTitle %>"?
		</h1>
	</div>
	<div class="delete-buttons">
		<button class="button">
			<a href="/dashboard">No</a>
		</button>
		<form action="/delete" method="POST">
			<input
				type="hidden"
				name="entryId"
				value="<%= entryId %>"
			/>
			<button type="submit" class="button">Yes</button>
		</form>
	</div>
</body>
